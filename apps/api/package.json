{
  "name": "@finintel/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "echo 'Add ESLint config'",
    "pretest": "prisma generate",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --config jest.config.cjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed:10k": "tsx prisma/seed.ts",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --config jest.config.cjs --coverage"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jose": "^5.8.0",
    "multer": "^1.4.5-lts.1",
    "rate-limiter-flexible": "^5.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "jest": "^29.7.0",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
